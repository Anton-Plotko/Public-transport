@page
@using Public_transport.Models
@model Public_transport.Pages.StopsModel
@{
}
<div class="text-center">
    <h1 class="display-6">Выберите остановку!</h1>
    <ul style="list-style: none;">
    @foreach (Stop Stop in Model.ResNameStops)
    {
        foreach(Relationship_between_transports_and_stops rel in Model.ResIdStops)
        {
        <li><a class="navbar-brand" href="/Time?IdStops=@Stop.Id&IdTransport=@rel.IdTransport">@Stop.Name</a></li>
        break;
        }
    }
    </ul>
</div>